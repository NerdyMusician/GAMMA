<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#111111" />
    <SolidColorBrush x:Key="PrimaryFontColor" Color="#ffffff" />
    <SolidColorBrush x:Key="TooltipFontColor" Color="#333333" />
    <SolidColorBrush x:Key="WindowBorder" Color="#444444" />

    <SolidColorBrush x:Key="ButtonBackground" Color="#222222" />
    <SolidColorBrush x:Key="ButtonBorder" Color="#333333" />
    <SolidColorBrush x:Key="ButtonHoverBackground" Color="#333333" />
    
    <SolidColorBrush x:Key="ScrollBarBackground" Color="#0d0d0d" />

    <SolidColorBrush x:Key="TrackerCardActive" Color="DarkGreen" />
    <SolidColorBrush x:Key="TrackerCardInactive" Color="#333333" />
    <SolidColorBrush x:Key="TrackerCardDead" Color="#000000" />
    <SolidColorBrush x:Key="TrackerCardSearchMatch" Color="DarkBlue" />
    <SolidColorBrush x:Key="TrackerCardOoc" Color="DarkRed" />

</ResourceDictionary>